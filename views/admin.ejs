<body>
  <% if(message!="") {%>
    <div class="alert alert-success"> 
      <%=message%>
    </div>
    <%}%>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark background">
    <div class="container">
      
      <a class="navbar-brand " href="#">Talented-Pak</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse navmain" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item navmain">
            <a class="btn btn-lg btn-outline-primary" class="nav-link active" aria-current="page" href="/addjob">Add Job</a>
          </li>
          <li class="nav-item navmain">
            <a class="btn btn-lg btn-outline-danger" class="nav-link active" aria-current="page" href="/addblog">Add Blog Post</a>
          </li>
          </ul>
          <form method="Post" action="/signout">
          <input type="submit" value="Logout" class="btn btn-lg btn-outline-danger" style="color: white; margin-left: 10px;" >
         </form>
          

      </div>
    </div>
  </nav>


<div class="container-fluid background pt-5 banner ">
   
      <figure class="text-center backgroundsty pt-5">
        <blockquote class="blockquote pt-5">
          <h1 style="font-size: 50px;">Welcome to Admin Panel</h1>
        </blockquote>
        <figcaption class="blockquote-footer backgroundsty pt-3" style="font-size: 30px;">
            Build,Develop and Rule the World with Your Hands <cite title="Source Title"></cite>
        </figcaption>
      </figure>
     
      
</div>


<% data.forEach(data=> { %>
    <div class="container mt-5">
      
      <div class="row mt-2 g-1">
        <div class="col-md-12">
          <div class="card p-2">
            <div class="text-right"> <small>
               
              </small> </div>

            <div class="text-center mt-2 p-3">  <span
                class="d-block font-weight-bold">
                
                <div class="container card mt-4" style="width: 18rem;">
                  <div class="card-body">
                    <h5 class="card-title"> <%= data.JOBNAME %></h5>
                    <h6 class="card-subtitle mb-2 text-muted"> <%= data.JOBTYPE %></h6>
                    <p class="card-text"> <%= data.PUBLISHER %>,  <%= data.CATEGORY %></p>
                    <a href="#" class="card-link" style="text-decoration:none;" >  <%= data.CITY %> </a>
                    <a href="#" class="card-link" style="text-decoration:none;"><%= data.DATE %></a>
                  </div>
                </div>
                 
              </span>
              <div class="d-flex flex-row align-items-center justify-content-center"> <i class="fa fa-map-marker"></i>
                <small class="ml-1">
                
                </small> </div>
                <div class="d-flex justify-content-between mt-3"> <span> 
                  </span> 
              <a href="/delete/<%= data.ID %>" class="btn btn-lg btn-outline-primary">Delete</a>
                  <!-- Modal -->
                   </div>
                  </span> 
                <a href="/edit/<%= data.ID %>" class="btn btn-lg btn-outline-primary">Edit</a>
                    <!-- Modal -->
                     </div>
                       
            </div>
          </div>
        </div>
      </div>
    </div>
    <% }) %>
