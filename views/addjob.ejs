<section class="vh-100 gradient-custom">
  <div class="container py-5 h-100">
    <div class="row justify-content-center align-items-center h-100">
      <div class="col-12 col-lg-9 col-xl-7">
        <div class="card shadow-2-strong card-registration" style="border-radius: 15px;">
          <div class="card-body p-4 p-md-5">
            <h3 class="mb-4 pb-2 pb-md-0 mb-md-5">Post a New Job</h3>
            <form method="POST" action="/addjob" enctype="multipart/form-data">
              <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input name="JOBNAME" type="text" id="JOBNAME" class="form-control form-control-lg" />
                    <label class="form-label" for="JOBNAME">Job Name</label>
                  </div>

                </div>
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input name="CATEGORY" type="text" id="CATEGORY" class="form-control form-control-lg" />
                    <label class="form-label" for="CATEGORY">CATEGORY</label>
                  </div>

                </div>
              </div>

              <div class="row">
                <div class="col-md-6 mb-4 d-flex align-items-center">

                  <div class="form-outline datepicker w-100">
                    <input name="CITY" type="text" class="form-control form-control-lg" id="CITY" />
                    <label for="CITY" class="form-label">CITY</label>
                  </div>
                  
                </div>
                <div class="col-md-6 mb-4 d-flex align-items-center">
                  <div class="form-outline datepicker w-100">
                    <input name="COUNTRY" type="text" class="form-control form-control-lg" id="COUNTRY" />
                    <label for="COUNTRY" class="form-label">COUNTRY</label>
                  </div>
                </div>

                <div class="col-md-6 mb-4 d-flex align-items-center">
                  <div class="form-outline datepicker w-100">
                    <input name="MIN_SAL" type="number" class="form-control form-control-lg" id="MIN_SAL" />
                    <label for="MIN_SAL" class="form-label">MINIMUN SALARY</label>
                  </div>
                 
                </div>
                <div class="col-md-6 mb-4 d-flex align-items-center">
                <div class="form-outline datepicker w-100">
                  <input name="MAX_SAL" type="number" class="form-control form-control-lg" id="MAX_SAL" />
                  <label for="MAX_SAL" class="form-label">MAXIMUM SALARY</label>
                </div>

              </div>

              <div class="col-md-6 mb-4 d-flex align-items-center">
                <div class="form-outline datepicker w-100">
                  <input name="JOBIMG" type="file" class="form-control form-control-lg" id="JOBIMG" />
                  <label for="JOBIMG" class="form-label">COMPANY LOGO</label>
                </div>

              </div>
              <div class="col-md-6 mb-4 d-flex align-items-center">
                <div class="form-outline datepicker w-100">
                  <input name = "JOBTYPE" type="text" class="form-control form-control-lg" id="JOBTYPE" />
                  <label for="JOBTYPE" class="form-label">JOB TYPE</label>
                </div>
</div>
              
                  <input class="btn btn-primary btn-lg" type="submit" value="Submit" />
                </div>
              </div>
              </div>

              
              
                
              </div>

             

              

            </form>
          </div>
        </div>
      </div>
    </div>
