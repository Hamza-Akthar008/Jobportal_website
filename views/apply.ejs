<section class="vh-100 gradient-custom">
  <div class="container py-5 h-100">
    <div class="row justify-content-center align-items-center h-100">
      <div class="col-12 col-lg-9 col-xl-7">
        <div class="card shadow-2-strong card-registration" style="border-radius: 15px;">
          <div class="card-body p-4 p-md-5">
            <h3 class="mb-4 pb-2 pb-md-0 mb-md-5">Apply for Job</h3>
            <form method="POST" action="/apply/<%=data[0].ID%>" enctype="multipart/form-data">
              <div class="row">
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input name="FNAME" type="text" id="FNAME" class="form-control form-control-lg" required />
                    <label class="form-label" for="FNAME">First Name</label>
                  </div>

                </div>
                <div class="col-md-6 mb-4">

                  <div class="form-outline">
                    <input name="LNAME" type="text" id="LNAME" class="form-control form-control-lg" required/>
                    <label class="form-label" for="LNAME">Last Name</label>
                  </div>

                </div>
              </div>

              <div class="row">
                <div class="col-md-6 mb-4 d-flex align-items-center">

                  <div class="form-outline datepicker w-100">
                    <input name="Contact" type="tel" class="form-control form-control-lg" id="Contact" required/>
                    <label for="Contact" class="form-label">Contact</label>
                  </div>
                  
                </div>
                

                <div class="col-md-6 mb-4 d-flex align-items-center">
                  <div class="form-outline datepicker w-100">
                    <input name="CV" type="file" class="form-control form-control-lg" id="CV" />
                    <label for="CV" class="form-label">CV</label>
                  </div>
                 
                </div>
                

           
              <div class="col-md-6 mb-4 d-flex align-items-center">
                <div class="form-outline datepicker w-100">
                  <input name = "Experience" type="number" class="form-control form-control-lg" id="Experience" required/>
                  <label for="Experience" class="form-label">Experience</label>
                </div>
</div>


              
                  <input class="btn btn-primary btn-lg" type="submit" value="Submit" />
                </div>
              </div>
              </div>

              
              
                
              </div>

             

              

            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>




